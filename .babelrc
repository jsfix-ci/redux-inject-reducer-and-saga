{
  "presets": [
    "react-app"
  ],
  "env": {
    "production": {
      "presets": [
        ["env", {
          "targets": {
            "ie": 10,
            "browsers": [
             "last 4 versions"
            ]
          },
          "forceAllTransforms": true
        }],
          "react-app"
      ]
    },
    // test environment transforms
    "test": {
      "presets": [
        ["env", {
          "targets": {
            "node": "current"
          }
        }],
        "react-app"
      ],
      "compact": false
    }
  }
}
