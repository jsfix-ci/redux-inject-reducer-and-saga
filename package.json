{
  "name": "redux-inject-reducer-and-saga",
  "version": "1.3.4",
  "description": "Inject reducer and saga anywhere in the application.",
  "main": "dist/index.js",
  "repository": "https://github.com/marcelmokos/redux-inject-reducer-and-saga.git",
  "author": "Marcel MokoÅ¡ (https://github.com/marcelmokos)",
  "keywords": [
    "react",
    "redux",
    "store",
    "create store",
    "injectable store",
    "reducer",
    "create reducer",
    "inject",
    "inject reducer",
    "inject saga",
    "inject redux-saga",
    "inject thunk",
    "inject redux-thunk"
  ],
  "license": "MIT",
  "engines": {
    "node": ">=6"
  },
  "scripts": {
    "flow:setup": "yarn && flow-typed install && flow-typed update",
    "flow": "flow --show-all-errors",
    "flow:coverage": "flow coverage ./src/index.js --color && flow-coverage-report -i 'src/**/*.js' -x 'src/**/*.test.js' -x 'src/**/*.spec.js' -t html",
    "prepush": "yarn test && yarn test:dist",
    "test": "jest",
    "test:watch": "yarn test --watch",
    "test:update": "yarn test --update",
    "test:coverage": "jest /src/ --coverage",
    "test:dist": "npm run prepublishOnly && jest /dist/ --coverage",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
    "lint": "eslint . --cache",
    "lint:fix": "yarn lint --fix",
    "lint:staged": "yarn lint:fix --max-warnings=0",
    "precommit": "lint-staged",
    "clean": "rimraf dist coverage",
    "build": "BABEL_ENV=production babel src --out-dir dist --copy-files",
    "prepublishOnly": "npm run clean && npm run build",
    "release-patch": "npm version patch -m \"Upgrade to %s\" && npm publish",
    "release-minor": "npm version minor -m \"Upgrade to %s\" && npm publish",
    "release-major": "npm version major -m \"Upgrade to %s\" && npm publish"
  },
  "lint-staged": {
    "*.{js,jsx}": [
      "lint:staged",
      "git add"
    ],
    "*.{json,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "history": "^4.7.2",
    "hoist-non-react-statics": "^2.3.1",
    "immutable": "^3.8.2",
    "invariant": "^2.2.2",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "5.0.0-alpha.6",
    "redux": "^3.7.2",
    "redux-immutable": "^4.0.0",
    "redux-saga": "^0.15.6",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-styled-components": "^1.2.1",
    "babel-preset-react-app": "^3.0.2",
    "coveralls": "^3.0.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.1",
    "enzyme-to-json": "^3.1.2",
    "eslint": "^4.8.0",
    "eslint-config-with-prettier": "^2.0.0",
    "flow-bin": "^0.56.0",
    "flow-coverage-report": "^0.3.0",
    "flow-typed": "^2.2.0",
    "husky": "^0.14.3",
    "jest": "^21.1.0",
    "lint-staged": "^4.2.3",
    "prettier": "^1.7.4",
    "raf": "^3.4.0",
    "react-test-renderer": "^16.0.0",
    "rimraf": "^2.6.2"
  }
}
